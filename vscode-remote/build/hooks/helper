#!/bin/bash

function Log() {
    local msg="`date '+%Y/%m/%d %H:%M:%S'` $1"
    echo ${msg}
}

function PrintOS() {
    cat /etc/issue || true
    cat /etc/*-release || true
}

function PrintEnv() {
    __SSH_PRIVATE=${SSH_PRIVATE}
    unset SSH_PRIVATE
    #########
    printenv
    #########
    export SSH_PRIVATE=${__SSH_PRIVATE}
    unset __SSH_PRIVATE
}